{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.+)",
      "has": [
        {
          "type": "host",
          "value": "www.bito.works"
        }
      ],
      "destination": "https://bito.works/$1",
      "permanent": true
    }
  ]
}